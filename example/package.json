{
  "name": "RNUMITestApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "watch": "umi g rn --dev",
    "start": "react-native start",
    "pree2e:android": "UMI_ENV=e2e umi g rn",
    "e2e:build-android": "cd android && ./gradlew assembleRelease",
    "e2e:run-android": "wdio ./android.wdio.config.js",
    "e2e:android": "yarn e2e:build-android && yarn e2e:run-android",
    "pree2e:ios": "UMI_ENV=e2e umi g rn",
    "e2e:build-ios": "cd ios && rm -rf build && /usr/bin/xcodebuild -quiet -sdk iphonesimulator -scheme RNUMITestApp -workspace RNUMITestApp.xcworkspace -configuration Release archive -archivePath \"build/RNUMITestApp.xcarchive\" CODE_SIGN_IDENTITY=\"\" CODE_SIGNING_REQUIRED=NO CODE_SIGNING_ALLOWED=NO",
    "e2e:run-ios": "wdio ./ios.wdio.config.js",
    "e2e:ios": "yarn e2e:build-ios && yarn e2e:run-ios",
    "haul": "haul start"
  },
  "dependencies": {
    "@ant-design/react-native": "3.3.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.5.1",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-gesture-handler": "1.6.1",
    "react-native-reanimated": "1.9.0",
    "react-native-safe-area-context": "3.0.2",
    "react-native-screens": "2.8.0",
    "umi-react-native-multibundle": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@haul-bundler/babel-preset-react-native": "^0.16.0",
    "@haul-bundler/cli": "^0.22.1",
    "@haul-bundler/preset-0.60": "^0.18.0",
    "@umijs/plugin-dva": "^0.7.2",
    "@wdio/appium-service": "^6.1.14",
    "@wdio/cli": "^6.1.15",
    "@wdio/jasmine-framework": "^6.1.14",
    "@wdio/local-runner": "^6.1.14",
    "@wdio/spec-reporter": "^6.1.14",
    "@wdio/sync": "^6.1.14",
    "appium": "^1.17.1",
    "metro-react-native-babel-preset": "^0.58.0",
    "umi": "^3.2.1",
    "umi-plugin-antd-react-native": "latest",
    "umi-preset-react-native": "latest",
    "umi-preset-react-navigation": "latest",
    "webdriverio": "^6.1.14"
  }
}
